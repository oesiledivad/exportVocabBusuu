{
    "manifest_version": 3,
    "name": "Busuu Vocabulary Downloader",
    "version": "1.0",
    "description": "Download Busuu vocabulary as Anki-compatible CSV + media files",
  "permissions": [
    "activeTab",
    "downloads",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  }
}